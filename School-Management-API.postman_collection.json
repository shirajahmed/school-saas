  "item": [
    {
      "name": "Complete Onboarding Flow",
      "item": [
        {
          "name": "1. School Registration",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"schoolName\": \"ABC International School\",\n  \"email\": \"admin@abcschool.com\",\n  \"phone\": \"+1234567890\",\n  \"password\": \"password123\",\n  \"address\": \"123 School Street, City\",\n  \"firstName\": \"John\",\n  \"lastName\": \"Admin\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/auth/school-signup",
              "host": ["{{baseUrl}}"],
              "path": ["auth", "school-signup"]
            }
          },
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 201) {",
                  "    const response = pm.response.json();",
                  "    pm.collectionVariables.set('schoolId', response.schoolId);",
                  "    pm.collectionVariables.set('onboardingToken', response.onboardingToken);",
                  "}"
                ]
              }
            }
          ]
        },
        {
          "name": "2. Get Onboarding Status",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/onboarding/status?token={{onboardingToken}}",
              "host": ["{{baseUrl}}"],
              "path": ["onboarding", "status"],
              "query": [
                {
                  "key": "token",
                  "value": "{{onboardingToken}}"
                }
              ]
            }
          }
        },
        {
          "name": "3. Step 1 - Basic School Info",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"token\": \"{{onboardingToken}}\",\n  \"step\": 1,\n  \"stepData\": {\n    \"schoolType\": \"HIGH_SCHOOL\",\n    \"language\": \"en\",\n    \"academicYearStart\": 4,\n    \"logo\": \"https://example.com/logo.png\"\n  }\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/onboarding/step",
              "host": ["{{baseUrl}}"],
              "path": ["onboarding", "step"]
            }
          }
        },
        {
          "name": "4. Step 2 - Subscription Selection",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"token\": \"{{onboardingToken}}\",\n  \"step\": 2,\n  \"stepData\": {\n    \"plan\": \"PROFESSIONAL\",\n    \"maxBranches\": 3,\n    \"features\": [\"attendance\", \"grades\", \"reports\", \"communication\"]\n  }\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/onboarding/step",
              "host": ["{{baseUrl}}"],
              "path": ["onboarding", "step"]
            }
          }
        },
        {
          "name": "5. Step 3 - Payment Processing",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"token\": \"{{onboardingToken}}\",\n  \"step\": 3,\n  \"stepData\": {\n    \"paymentMethod\": \"stripe\",\n    \"paymentToken\": \"tok_visa\"\n  }\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/onboarding/step",
              "host": ["{{baseUrl}}"],
              "path": ["onboarding", "step"]
            }
          }
        },
        {
          "name": "6. Complete Onboarding",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"token\": \"{{onboardingToken}}\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/onboarding/complete",
              "host": ["{{baseUrl}}"],
              "path": ["onboarding", "complete"]
            }
          }
        },
        {
          "name": "7. Login to Dashboard",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"identifier\": \"admin@abcschool.com\",\n  \"password\": \"password123\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/auth/login",
              "host": ["{{baseUrl}}"],
              "path": ["auth", "login"]
            }
          },
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 200) {",
                  "    const response = pm.response.json();",
                  "    pm.collectionVariables.set('accessToken', response.accessToken);",
                  "    pm.collectionVariables.set('refreshToken', response.refreshToken);",
                  "}"
                ]
              }
            }
          ]
        }
      ]
    },
